<div class="container panel">
	<div class="spacer"></div>
	<div class="row">
		<div class="span10 offset1">
			<div class="well">
				<p id="account-error">
					This <%= errorViewModel.courseType %> is not available for registration.
					<% if(errorViewModel.email) { %>
					<br/><br/>
					Please contact the <%= errorViewModel.courseType %> trainer <a href="mailto:<%=errorViewModel.email%>">
						<%=errorViewModel.email%></a> for more information.
					<% } %>
					<br/><br/>
					<a href="/">Return to log in</a>
				</p>
			</div>
		</div>
	</div>
</div>
